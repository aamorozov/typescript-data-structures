{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "semicolon": [true],
    "member-access": [false],
    "ordered-imports": [false],
    "no-console": [false],
    "no-var-requires": true,
    "quotemark": [true, "double"],
    "space-in-parens": [false, "never"],
    "object-curly-spacing": [false, "never"],
    "array-bracket-spacing": [false, "never"],
    "jsx-no-lambda": [false],
    "max-classes-per-file": [false, 1],
    "prefer-for-of": false
  },
  "rulesDirectory": []
}
